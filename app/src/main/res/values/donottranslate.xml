        <item>qwertz</item>
        <item>da:AsciiCapable</item>
        <item>nordic</item>
        <item>de:AsciiCapable</item>
        <item>qwertz</item>
        <item>es:AsciiCapable</item>
        <item>spanish</item>
        <item>fi:AsciiCapable</item>
        <item>nordic</item>
        <item>fr:AsciiCapable</item>
        <item>azerty</item>
        <item>fr_CA:AsciiCapable</item>
        <item>qwerty</item>
        <item>hr:AsciiCapable</item>
