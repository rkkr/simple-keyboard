<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <switch>
        <case latin:showExtraChars="true">
            <!-- U+0430: "а" CYRILLIC SMALL LETTER A -->
            <Key
                latin:keySpec="&#x0430;"
                latin:keyHintLabel="\@"
                latin:additionalMoreKeys="\\@" />
            <!-- U+0441: "с" CYRILLIC SMALL LETTER ES -->

            <Key
                latin:keySpec="&#x0441;"
                latin:keyHintLabel="#"
                latin:additionalMoreKeys="#"
            />
            <!-- U+0434: "д" CYRILLIC SMALL LETTER DE -->
            <Key
                latin:keySpec="&#x0434;"
                latin:keyHintLabel="$"
                latin:additionalMoreKeys="$" />
            <!-- U+0444: "ф" CYRILLIC SMALL LETTER EF -->
            <Key
                latin:keySpec="&#x0444;"
                latin:keyHintLabel="%"
                latin:additionalMoreKeys="%" />
            <!-- U+0433: "г" CYRILLIC SMALL LETTER GHE -->
            <Key
                latin:keySpec="&#x0433;"
                latin:keyHintLabel="&amp;"
                latin:additionalMoreKeys="&amp;"
                latin:moreKeys="&#x0491;" />
            <!-- U+0445: "х" CYRILLIC SMALL LETTER HA -->
            <Key
                latin:keySpec="&#x0445;"
                latin:keyHintLabel="_"
                latin:additionalMoreKeys="_" />
            <!-- U+0439: "й" CYRILLIC SMALL LETTER SHORT I -->
            <Key
                latin:keySpec="&#x0439;"
                latin:keyHintLabel="/"
                latin:additionalMoreKeys="/"
                latin:moreKeys="&#x0457;" />
            <!-- U+043A: "к" CYRILLIC SMALL LETTER KA -->
            <Key
                latin:keySpec="&#x043A;"
                latin:keyHintLabel="-"
                latin:additionalMoreKeys="-" />
            <!-- U+043B: "л" CYRILLIC SMALL LETTER EL -->
            <Key
                latin:keySpec="&#x043B;"
                latin:keyHintLabel="+"
                latin:additionalMoreKeys="+" />
            <!-- U+0448: "ш" CYRILLIC SMALL LETTER SHA -->
            <Key
                latin:keySpec="&#x0448;"
                latin:keyHintLabel="("
                latin:additionalMoreKeys="(" />
            <Key
                latin:keySpec="!text/keyspec_east_slavic_row1_9"
                latin:keyHintLabel=")"
                latin:additionalMoreKeys=")" />
        </case>
        <default>
            <!-- U+0430: "а" CYRILLIC SMALL LETTER A -->
            <Key
                latin:keySpec="&#x0430;" />
            <!-- U+0441: "с" CYRILLIC SMALL LETTER ES -->

            <Key
                latin:keySpec="&#x0441;"
            />
            <!-- U+0434: "д" CYRILLIC SMALL LETTER DE -->
            <Key
                latin:keySpec="&#x0434;" />
            <!-- U+0444: "ф" CYRILLIC SMALL LETTER EF -->
            <Key
                latin:keySpec="&#x0444;"
            />
            <!-- U+0433: "г" CYRILLIC SMALL LETTER GHE -->
            <Key
                latin:keySpec="&#x0433;" />
            <!-- U+0445: "х" CYRILLIC SMALL LETTER HA -->
            <Key
                latin:keySpec="&#x0445;" />
            <!-- U+0439: "й" CYRILLIC SMALL LETTER SHORT I -->
            <Key
                latin:keySpec="&#x0439;"
                latin:moreKeys="&#x0457;" />
            <!-- U+043A: "к" CYRILLIC SMALL LETTER KA -->
            <Key
                latin:keySpec="&#x043A;" />
            <!-- U+043B: "л" CYRILLIC SMALL LETTER EL -->
            <Key
                latin:keySpec="&#x043B;" />
            <!-- U+0448: "ш" CYRILLIC SMALL LETTER SHA -->
            <Key
                latin:keySpec="&#x0448;" />
            <Key
                latin:keySpec="!text/keyspec_east_slavic_row1_9"
            />
        </default>
    </switch>
</merge>
